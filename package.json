{
  "name": "lilypond-ui",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/main.bundle.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/doches/lilypond-ui.git"
  },
  "scripts": {
    "clean": "rimraf dist && rimraf artifacts && rimraf 'LilyPond Editor-darwin-x64'",
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && webpack --watch --config ./webpack.dev.js --progress --colors",
    "build": "npm-run-all clean make-app dmg",
    "make-app": "npm-run-all prod package package-icon",
    "prod": "webpack --config ./webpack.prod.js --progress --colors",
    "package": "electron-packager . --overwrite",
    "package-icon": "cp ./artwork/icons.icns './LilyPond Editor-darwin-x64/LilyPond Editor.app/Contents/Resources/electron.icns'",
    "dmg": "electron-installer-dmg --overwrite --icon=artwork/icons.icns --title='LilyPond Editor' --out=./artifacts './LilyPond Editor-darwin-x64/LilyPond Editor.app' 'LilyPond Editor'"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/electron-settings": "^3.1.1",
    "@types/lodash": "^4.14.123",
    "@types/mousetrap": "^1.6.1",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-js-pagination": "^3.0.2",
    "css-loader": "^2.1.1",
    "electron": "4.1.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.14.1",
    "classnames": "^2.2.6",
    "codemirror": "^5.44.0",
    "codemirror-atom-modes": "^1.1.1",
    "electron-settings": "^3.2.0",
    "lodash": "^4.17.11",
    "mousetrap": "^1.6.3",
    "react": "^16.8.4",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.8.4",
    "react-js-pagination": "^3.0.2",
    "react-pdf": "^4.0.5",
    "react-split-pane": "^0.1.85"
  },
  "productName": "LilyPond Editor"
}
